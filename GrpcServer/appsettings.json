{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Debug",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Https/2": {
        "Url": "https://*:5001",
        "Protocols": "Http2"
      },
      "Http/2": {
        "Url": "http://*:5000",
        "Protocols": "Http2"
      },
      "Https/1": {
        "Url": "https://*:6001",
        "Protocols": "Http1"
      }
    }
  }
}